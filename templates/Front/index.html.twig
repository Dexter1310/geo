{% extends 'base.html.twig' %}
{% block title %}HOME{% endblock %}
{% block body %}
    <div class="container">
        <hr>
        <h2 id="select-info">Selecciona destino</h2>
        <form method="post" name="geo">
            <div class="row">
                <div class="form-group col-md-5 col-sm-12">
                    <label>Provincia</label>
                    <select class="form-control provi2" name="pro" required>
                        {% for pro in provincia %}
                            <option value="{{ pro.id }}">{{ pro.nm }}</option>
                        {% endfor %}
                    </select>
                </div>
                <div class="form-group col-md-5 col-sm-12">
                    <label>Municipio</label>
                    <select class="form-control muniSelect" name="muniSelect" required>
                        <option disabled selected value="">Selecciona Provincia</option>
                    </select>
                </div>
                <div class="form-group col-md-2 col-sm-12 mt-2">
                    <label></label>
                    <div>
                        <button type="submit" class="btn btn-block btn-dark ">Informaci√≥n</button>
                    </div>
                </div>
            </div>
        </form>
        <div class="container-fluid" id="info-geo">
        </div>
    </div>

{% endblock %}